{
  "build": {
    "env": {
      "VITE_API_BASE_URL": "@vite_api_base_url"
    }
  },
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ]
} 