{
  "build": {
    "env": {
      "VITE_API_BASE_URL": "https://charterhub-api.onrender.com",
      "VITE_API_URL": "https://charterhub-api.onrender.com",
      "VITE_PHP_API_URL": "https://charterhub-api.onrender.com",
      "VITE_WP_API_URL": "https://yachtstory.com/wp-json",
      "VITE_USE_JWT": "true",
      "VITE_FRONTEND_URL": "https://app.yachtstory.be",
      "VITE_ALLOWED_ORIGINS": "https://app.yachtstory.be,https://admin.yachtstory.be",
      "VITE_ENV": "production",
      "VITE_DEBUG": "false",
      "TSC_COMPILE_ON_ERROR": "true",
      "VITE_SKIP_TS_CHECK": "true",
      "NODE_ENV": "production"
    }
  },
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/api/(.*)",
      "dest": "https://charterhub-api.onrender.com/api/$1"
    },
    {
      "src": "^/auth/(.*)",
      "dest": "https://charterhub-api.onrender.com/auth/$1"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ]
} 