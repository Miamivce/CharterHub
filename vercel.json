{
  "buildCommand": "cd frontend && npm install vite@5.4.14 @vitejs/plugin-react@4.3.4 --save-dev && node build.cjs",
  "installCommand": "echo Skipping global install step",
  "outputDirectory": "frontend/dist",
  "framework": "vite",
  "devCommand": "npm run dev",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "SKIP_TYPESCRIPT_CHECK": "true",
    "VITE_SKIP_TS_CHECK": "true",
    "TSC_COMPILE_ON_ERROR": "true"
  }
} 